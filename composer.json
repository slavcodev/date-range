{
  "name": "zeeproject/date-range",
  "description": "Date range implementation",
  "license": "BSD-2-Clause",
  "homepage": "https://github.com/zee/date-range",
  "keywords": [
    "date range"
  ],
  "authors": [
    {
      "name": "Veaceslav Medvedev",
      "email": "slavcopost@gmail.com",
      "homepage": "https://github.com/slavcodev"
    },
    {
      "name": "All awesome contributors",
      "homepage": "https://github.com/zee/exceptions/graphs/contributors"
    }
  ],
  "config": {
    "sort-packages": true
  },
  "require": {
    "php": "^7.1",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.4",
    "scrutinizer/ocular": "~1.1"
  },
  "autoload": {
    "psr-4": {
      "Zee\\DateRange\\": ["src/"]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Zee\\DateRange\\": ["tests/"]
    }
  },
  "scripts": {
    "check": [
      "@test"
    ],
    "test": "phpunit",
    "test-coverage": "phpunit --coverage-clover build/logs/clover.xml",
    "show-coverage": "phpunit --coverage-clover build/logs/clover.xml --coverage-text=php://stdout"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
